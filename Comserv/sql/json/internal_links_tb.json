{
  "table_name": "internal_links_tb",
  "description": "Stores internal links for navigation across different site sections",
  "columns": {
    "id": {
      "data_type": "integer",
      "is_auto_increment": true,
      "is_nullable": false,
      "description": "Primary key for the internal links table"
    },
    "category": {
      "data_type": "varchar",
      "size": 50,
      "is_nullable": false,
      "description": "Category of the link (e.g., Main_links, Member_links, Admin_links, Hosted_link)"
    },
    "sitename": {
      "data_type": "varchar",
      "size": 50,
      "is_nullable": false,
      "description": "Site name the link belongs to (or 'All' for links that appear on all sites)"
    },
    "name": {
      "data_type": "varchar",
      "size": 100,
      "is_nullable": false,
      "description": "Display name of the link"
    },
    "url": {
      "data_type": "varchar",
      "size": 255,
      "is_nullable": false,
      "description": "URL or path for the link"
    },
    "target": {
      "data_type": "varchar",
      "size": 20,
      "is_nullable": true,
      "default_value": "_self",
      "description": "Target attribute for the link (e.g., _self, _blank)"
    },
    "description": {
      "data_type": "text",
      "is_nullable": true,
      "description": "Description of the link for administrative purposes"
    },
    "link_order": {
      "data_type": "integer",
      "is_nullable": true,
      "default_value": 0,
      "description": "Order in which the link should appear in the navigation"
    },
    "status": {
      "data_type": "integer",
      "is_nullable": true,
      "default_value": 1,
      "description": "Status of the link (1 = active, 0 = inactive, 2 = featured)"
    },
    "created_at": {
      "data_type": "datetime",
      "is_nullable": true,
      "default_value": "CURRENT_TIMESTAMP",
      "description": "Timestamp when the link was created"
    },
    "updated_at": {
      "data_type": "datetime",
      "is_nullable": true,
      "default_value": "CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP",
      "description": "Timestamp when the link was last updated"
    }
  },
  "primary_key": ["id"],
  "indexes": [
    {
      "name": "idx_internal_links_category",
      "columns": ["category"]
    },
    {
      "name": "idx_internal_links_sitename",
      "columns": ["sitename"]
    },
    {
      "name": "idx_internal_links_status",
      "columns": ["status"]
    }
  ],
  "relationships": [],
