{
  "table_name": "page_tb",
  "description": "Stores page information for site navigation and content management",
  "columns": {
    "id": {
      "data_type": "integer",
      "is_auto_increment": true,
      "is_nullable": false,
      "description": "Primary key for the page table"
    },
    "menu": {
      "data_type": "varchar",
      "size": 50,
      "is_nullable": false,
      "description": "Menu category the page belongs to (e.g., Main, member, Admin)"
    },
    "sitename": {
      "data_type": "varchar",
      "size": 50,
      "is_nullable": false,
      "description": "Site name the page belongs to (or 'All' for pages that appear on all sites)"
    },
    "name": {
      "data_type": "varchar",
      "size": 100,
      "is_nullable": false,
      "description": "Display name of the page"
    },
    "url": {
      "data_type": "varchar",
      "size": 255,
      "is_nullable": false,
      "description": "URL or path for the page"
    },
    "target": {
      "data_type": "varchar",
      "size": 20,
      "is_nullable": true,
      "default_value": "_self",
      "description": "Target attribute for the page link (e.g., _self, _blank)"
    },
    "description": {
      "data_type": "text",
      "is_nullable": true,
      "description": "Description of the page for administrative purposes"
    },
    "link_order": {
      "data_type": "integer",
      "is_nullable": true,
      "default_value": 0,
      "description": "Order in which the page should appear in the navigation"
    },
    "status": {
      "data_type": "integer",
      "is_nullable": true,
      "default_value": 1,
      "description": "Status of the page (1 = active, 0 = inactive, 2 = featured)"
    },
    "created_at": {
      "data_type": "datetime",
      "is_nullable": true,
      "default_value": "CURRENT_TIMESTAMP",
      "description": "Timestamp when the page was created"
    },
    "updated_at": {
      "data_type": "datetime",
      "is_nullable": true,
      "default_value": "CURRENT_TIMESTAMP ON UPDATE CURRENT_TIMESTAMP",
      "description": "Timestamp when the page was last updated"
    }
  },
  "primary_key": ["id"],
  "indexes": [
    {
      "name": "idx_page_menu",
      "columns": ["menu"]
    },
    {
      "name": "idx_page_sitename",
      "columns": ["sitename"]
    },
    {
      "name": "idx_page_status",
      "columns": ["status"]
    }
  ],
  "relationships": [],
