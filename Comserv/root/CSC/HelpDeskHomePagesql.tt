 [% FOREACH link = c.stash.dbi.query("SELECT * FROM page_tb
                                  WHERE  (status  = 2 AND
                                         page_code = 'HelpDeskHomeView')
                                  ORDER BY sitename
                                  ")%]
               [% site      = link.sitename        %]
                 [% PageName  = link.view_name %]
                 [% Record_id = link.record_id  %]
                 [% body      = link.body %]
                 [% status      = link.status %]
                 [%  page_code     = link.page_code %]
                 [% lastupdate = link.lastupdate %]
       [% END %]
