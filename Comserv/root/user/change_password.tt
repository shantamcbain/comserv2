[% META title = 'change_password' %]
[% PageVersion = 'change_password.tt,v 0.01 2024/08/09 shanta Exp shanta ' %]
[% IF debug_mode == 1 %]
  [% PageVersion %]
    [% INCLUDE 'debug.tt' %]
